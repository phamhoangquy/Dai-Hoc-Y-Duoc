section.team-doctor
	.container
		.section-title
			span Đội ngũ bác sĩ
			em.mdi.mdi-gamepad
				span
				span
		.row
			.col(class="w-full lg:w-3/12")
				.section-list
					.list-header Chuyên khoa
					.list-body
